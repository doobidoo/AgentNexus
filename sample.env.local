# Sample environment file for Agent Nexus
# Copy this file to .env.local and fill in your API keys

# Default Model Provider (openai, anthropic, gemini, ollama)
DEFAULT_MODEL_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=
OPENAI_COMPLETION_MODEL=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-large

# Anthropic Configuration
ANTHROPIC_API_KEY=
ANTHROPIC_COMPLETION_MODEL=claude-3-7-sonnet-20250219

# Google Gemini Configuration
GEMINI_API_KEY=
GEMINI_COMPLETION_MODEL=gemini-1.5-pro
GEMINI_EMBEDDING_MODEL=embedding-001

# Ollama Configuration
ENABLE_OLLAMA=false
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_COMPLETION_MODEL=llama3
OLLAMA_EMBEDDING_MODEL=llama3

# Weaviate Configuration
ENABLE_WEAVIATE=false
WEAVIATE_HOST=localhost:8080
WEAVIATE_SCHEME=http
WEAVIATE_API_KEY=
WEAVIATE_INDEX_NAME=AgentNexus

# Tool Configuration
NEXUS_TOOL_VECTORSEARCH_INDEXNAME=AgentNexus
NEXUS_TOOL_TEXTGENERATION_MODEL=default
